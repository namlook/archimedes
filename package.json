{
  "name": "archimedes",
  "version": "0.0.12",
  "description": "A simple ORDFM",
  "main": "lib",
  "repository": "https://github.com/namlook/archimedes",
  "scripts": {
    "prepublish": "npm run build",
    "lint": "eslint src tests",
    "build": "eslint src tests && rm -rf lib ; babel src --optional runtime --out-dir lib",
    "watch": "babel src --watch --optional runtime --out-dir lib --source-maps",
    "test": "eslint src tests && lab -T tests/transform.js tests -vSl",
    "coverage": "lab -T tests/transform.js -r html -o coverage.html tests"
  },
  "files": [
    "lib"
  ],
  "keywords": [],
  "author": "Namlook",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.25",
    "babel-core": "^5.8.25",
    "chalk": "^1.1.0",
    "code": "^1.5.0",
    "eslint": "^0.24.1",
    "lab": "^5.18.1",
    "source-map-support": "^0.3.2"
  },
  "dependencies": {
    "JSONStream": "0.10.0",
    "babel-runtime": "5.8.25",
    "bluebird": "2.10.2",
    "csv": "0.4.6",
    "event-stream": "3.3.2",
    "inflected": "1.1.6",
    "joi": "6.7.1",
    "lodash": "3.10.1",
    "moment": "2.10.6",
    "n3": "0.4.3",
    "querystring": "0.2.0",
    "sparqljs": "git://github.com/namlook/SPARQL.js.git",
    "stream-stream": "1.2.6",
    "uuid": "2.0.1",
    "wreck": "6.3.0"
  }
}
