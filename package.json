{
  "name": "archimedes",
  "version": "0.0.1",
  "description": "A simple ORDFM",
  "main": "lib",
  "scripts": {
    "preinstall": "npm install coffee-script && npm run build",
    "build": "coffee -co lib src/",
    "live": "coffee -wco lib src/",
    "test": "npm run build && mocha lib/test/model lib/test/database",
    "coverage": "npm run build && mocha -r blanket -R html-cov lib/test/database lib/test/model > coverage.html && open coverage.html"
  },
  "keywords": [],
  "author": "Namlook",
  "license": "MIT",
  "config": {
    "blanket": {
      "pattern": [
        "lib/interface",
        "lib/rdf"
      ]
    }
  },
  "devDependencies": {
    "chai": "~1.8.1",
    "mocha": "~1.14.0",
    "sinon": "~1.7.3",
    "coffeeify": "~0.6.0",
    "blanket": "^1.1.6"
  },
  "dependencies": {
    "underscore": "^1.6.0",
    "underscore.string": "~2.3.3",
    "objectdiff": "~1.1.0",
    "validator": "~3.1.0",
    "async": "~0.2.9",
    "extendonclass": "~1.0.0",
    "request": "~2.33.0",
    "superagent": "^0.17.0"
  }
}
