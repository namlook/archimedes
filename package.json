{
  "name": "archimedes",
  "version": "0.0.9",
  "description": "A simple ORDFM",
  "main": "lib",
  "repository": "https://github.com/namlook/archimedes",
  "scripts": {
    "postinstall": "babel src --optional runtime --out-dir lib",
    "lint": "eslint src tests",
    "build": "eslint src tests && babel src --optional runtime --out-dir lib --source-maps",
    "watch": "babel src --watch --optional runtime --out-dir lib --source-maps",
    "test": "eslint src tests && lab -T tests/transform.js tests -vSl",
    "coverage": "lab -T tests/transform.js -r html -o coverage.html tests"
  },
  "files": [
    "src"
  ],
  "keywords": [],
  "author": "Namlook",
  "license": "MIT",
  "devDependencies": {
    "chalk": "^1.1.0",
    "code": "^1.5.0",
    "eslint": "^0.24.1",
    "lab": "^5.18.1",
    "source-map-support": "^0.3.2"
  },
  "dependencies": {
    "babel": "^5.8.25",
    "babel-core": "^5.8.25",
    "babel-runtime": "^5.8.25",
    "bluebird": "^2.10.2",
    "csv": "^0.4.6",
    "inflected": "^1.1.6",
    "joi": "^6.7.1",
    "lodash": "3.10.1",
    "moment": "^2.10.6",
    "n3": "^0.4.3",
    "querystring": "^0.2.0",
    "sparqljs": "git://github.com/namlook/SPARQL.js.git",
    "uuid": "^2.0.1",
    "wreck": "^6.3.0"
  }
}
